<!DOCTYPE html>
<html lang="en">
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>

    <% articles.forEach((article) => { %>
    <div>
      <p><b>title :</b> <%= article.title %></p>
      <p><b>body :</b> <%= article.body %></p>
      <p>------------------------------------</p>
    </div>
    <%})%>
  </body>
</html>
